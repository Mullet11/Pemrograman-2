<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/21" 
            xmlns:fx="http://javafx.com/fxml/1" 
            fx:controller="Controller.Controller"
            prefHeight="400.0" prefWidth="600.0">
   <children>
      <TableView fx:id="tableMahasiswa" 
                 layoutX="50.0" layoutY="50.0" 
                 prefHeight="300.0" prefWidth="500.0"
                 AnchorPane.topAnchor="50.0"
                 AnchorPane.leftAnchor="50.0"
                 AnchorPane.rightAnchor="50.0"
                 AnchorPane.bottomAnchor="50.0">
        <columns>
          <TableColumn fx:id="colNIM" prefWidth="150.0" text="NIM" />
          <TableColumn fx:id="colNama" prefWidth="150.0" text="Nama" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
